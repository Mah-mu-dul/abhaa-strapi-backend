{
  "kind": "collectionType",
  "collectionName": "tfi_authors",
  "info": {
    "singularName": "tfi-author",
    "pluralName": "tfi-authors",
    "displayName": "TFI Author"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "bio": {
      "type": "text"
    },
    "profile_img": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "join_date": {
      "type": "date"
    },
    "current_status": {
      "type": "enumeration",
      "enum": [
        "Active",
        "Inactive"
      ]
    },
    "tfi_blogs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tfi-blog.tfi-blog",
      "mappedBy": "tfi_author"
    }
  }
}
